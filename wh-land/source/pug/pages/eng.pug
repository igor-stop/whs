extends ../main/base

block variables
	- let title = 'English';
	-
		let jobs = [
			{
				percent: 59,
				title: 'Наука, образование'
			},
			{
				percent: 59,
				title: 'Госслужба'
			},
			{
				percent: 54,
				title: 'Закупки'
			},
			{
				percent: 47,
				title: 'IT, телекомуникации'
			},
			{
				percent: 37,
				title: 'Административный персонал'
			},
			{
				percent: 36,
				title: 'Юристы'
			},
			{
				percent: 33,
				title: 'Маркетинг'
			},
			{
				percent: 30,
				title: 'ТОП - менеджмент'
			},
			{
				percent: 25,
				title: 'Управление персоналом'
			},
			{
				percent: 25,
				title: 'Бухгалтерия, финансы, предприятия'
			},
			{
				percent: 16,
				title: 'Банковская сфера'
			},
			{
				percent: 14,
				title: 'Продажи'
			}
		];
block content
	section.enFirst
		.enContainer
			img(src="img/logo-marathon.svg" alt="").enFirst__logo
			.enFirst__row
				.enFirst__left
					h1.enFirst__title Технический английский
					p.enFirst__for для программистов
					p.enFirst__description Добавь графу "Технический английский" в свое резюме, и <b>через 1,5 месяца</b> обучения, <b>получай в 2 раза больше</b> предложений о работе.
					img(src="img/enStart.svg" alt="").enFirst__img--mobile
					a(href="http://lk.webheroschool.ru/tech_eng_pay", onclick="yaCounter52985686.reachGoal('goToPayment'); return true;").enButton Записаться
					.enFirst__start
						img(src="img/start-up.svg", alt="").enFirst__startIcon
						.enFirst__startText
							p Старт потока 1 мая
							span количество мест ограничено!
				.enFirst__right
					img(src="img/enStart.svg" alt="").enFirst__img
	
	section.enGlish
		.enGlish__stats
			img(src="img/stats.png" alt="").enGlish__statsImg
			.enGlish__statsText вакансий в сфере IT и телекомуникации, в которых требуется владеть техническим английским языком
			.enGlish__under (Статистика вакансий c hh.ru)
		.enContainer
			.title 
				span.title__text Количество вакансий с требованием владеть английским языком
			.enGlish__subtitle Статистика вакансий c hh.ru
			each item, i in jobs
				.enGlish__row
					.enGlish__left.enGlish__side
						.enGlish__li= item.title
					.enGlish__right.enGlish__side
						if (i == 3)
							.enGlish__digitsLi.enGlish__digitsLi--active(style=`width: ${item.percent}%`, data-value=`${item.percent}%`)
						else
							.enGlish__digitsLi(style=`width: ${item.percent}%`, data-value=`${item.percent}%`)
	section.enBefore
		.enContainer
			.title 
				span.title__text Твоя жизнь до курса
			.enBefore__row
				.enBefore__left
					img(src="img/enBefore.svg", alt="").enBefore__img
				.enBefore__right
					ul.enBefore__ul
						li.enBefore__li <b>Сложности в самостоятельном обучении</b> по статьям и видео-урокам на английском языке
						li.enBefore__li <b>Неспособность быть в тренде</b>, ведь все актуальные новости, обновления, статьи  и книги сначала выходят на английском языке.
						li.enBefore__li <b>Возможность упустить что-то важное.</b> Многие обсуждения трендов и технологий ведутся в социальных сетях или форумах на английском языке. Например, в твиттере или stackoverflow. 
						li.enBefore__li <b>Ограничения по карьерному росту.</b> Для многих команд и компаний очень важно, чтобы вы могли работать с технической английской литературой, вести переговоры с англоговорящими заказчиками и без проблем самостоятельно разбирались с новыми библиотеками, программами и функционалом на английском языке. 
						li.enBefore__li <b>Ограничения по поиску работы.</b> С пунктом "навык работы с технической литературой" тебе доступно меньше вакансий с интересными предложениями. 
						li.enBefore__li <b>Возможные ошибки.</b> Отсутствие знаний английского языка не только ограничивают наши возможности в чем-то, но <b>могут</b> и <b>нанести вред твоей репутации и карьере.</b> Очень маленькая ошибка в рамках огромного продукта может стоить миллионы рублей. Нелепый перевод, неуместное использование имен переменных или критичные ошибки с вызовом не того метода или функции могут подорвать твою репутацию. 
	section.enAfter
		.enContainer
			.title 
				span.title__text Твоя жизнь до курса
			.enAfter__row
				.enAfter__left
					ul.enAfter__ul
						li.enAfter__li
							.enAfter__icon
							p.enAfter__text Возможность <b>обучаться быстрее,</b> по самым актуальным статьям и книгам на английском языке. 
						li.enAfter__li
							.enAfter__icon
							p.enAfter__text У тебя <b>увеличится круг знакомств.</b> Теперь ты сможешь заводить друзей, коллег и знакомых со всего мира! 

						li.enAfter__li
							.enAfter__icon
							p.enAfter__text Ты больше <b>не совершишь критически важных ошибок,</b> которые навредят твоей карьере и продукту.

						li.enAfter__li
							.enAfter__icon
							p.enAfter__text Ты сможешь начать <b>работать с зарубежными заказами,</b> работать удаленно на зарубежные компании и получать зарплату в долларах.

						li.enAfter__li
							.enAfter__icon
							p.enAfter__text Количество <b>доступных вакансий</b> на рынке <b>увеличится в разы</b> из-за нового пункта в резюме "Навык работы с технической литературой на английском языке".


				.enAfter__right
					img(src="img/enAfter.svg", alt="").enAfter__img

